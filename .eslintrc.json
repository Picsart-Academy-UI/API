{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends":  ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "consistent-return": "off",
    "padded-blocks": "off",
    "no-trailing-spaces": "off",
    "import/prefer-default-export": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "prefer-destructuring": ["error", {
      "object": true,
       "array": false
    }],
    "no-unused-vars": "off"
  }
}
